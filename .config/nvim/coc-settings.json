{
    "colors.filetypes": [
        "*"
    ],
    "[cpp][c]": {
        "inlayHint.enable": false
    },
    "Lua.diagnostics.globals": ["vim"],
    "Lua.format.defaultConfig": {
        "indent_style": "space",
        "indent_size": "2"
    },
    "Lua.format.enable": true,
    "clangd.disableProgressNotifications": true,
    "clangd.arguments": [
        "--all-scopes-completion",
        "--background-index",
        "--cross-file-rename",
        "--suggest-missing-includes"
    ],
    "coc.preferences.enableMarkdown": true,
    "suggest.defaultSortMethod": "length",
    "snippets.textmateSnippetsRoots": [
        "/Users/bytedance/Library/Application Support/Code - Insiders/User/snippets"
    ],
    "hover.autoHide": true,
    "hover.target": "float",
    "explorer.width": 50,
    "explorer.file.showHiddenFiles": true,
    "explorer.icon.enableNerdfont": true,
    "explorer.keyMappings.global": {
        "<space>": "toggleSelection"
    },
    "coc.preferences.formatOnSaveFiletypes": [
        "javascript",
        "typescript"
    ],
    "prettier.eslintIntegration": false,
    "tree.closedIcon": "",
    "tree.openedIcon": "",
    "javascript.format.semicolons": "insert",
    "http.proxyStrictSSL": false,
    "sh.enable": true,
    "cSpell.userWords": [
        "Jsep",
        "Neovim",
        "PCONNECTIONSTATUS",
        "RXJSON",
        "Ufrag",
        "Uncapitalize",
        "Vpaas",
        "absl",
        "allcore",
        "cmake",
        "gruvbox",
        "nvim",
        "sdes",
        "sonokai",
        "srflx",
        "srtp",
        "test"
    ],
    "codeLens.enable": true,
    "diagnostic.infoSign": "",
    "diagnostic.warningSign": "",
    "diagnostic.errorSign": "",
    "diagnostic.hintSign": "",
    "diagnostic.enableMessage": "jump",
    "diagnostic.virtualText": true,
    "diagnostic.virtualTextCurrentLineOnly": true,
    "diagnostic-languageserver.filetypes": {
        "sh": "shellcheck",
        "vim": "vint"
    },
    "diagnostic-languageserver.formatFiletypes": {
        "sh": "shfmt"
    },
    "diagnostic-languageserver.formatters": {
        "shfmt": {
            "command": "shfmt",
            "args": [
                "-bn",
                "-i",
                "2",
                "-"
            ]
        }
    },
    "explorer.presets": {
        ".vim": {
            "root-uri": "~/.config/nvim"
        },
        "cocConfig": {
            "root-uri": "~/.config/coc"
        },
        "tab": {
            "position": "tab",
            "quit-on-open": true
        },
        "floating": {
            "position": "floating",
            "open-action-strategy": "sourceWindow"
        },
        "floatingTop": {
            "position": "floating",
            "floating-position": "center-top",
            "open-action-strategy": "sourceWindow"
        },
        "floatingLeftside": {
            "position": "floating",
            "floating-position": "left-center",
            "floating-width": 50,
            "open-action-strategy": "sourceWindow"
        },
        "floatingRightside": {
            "position": "floating",
            "floating-position": "right-center",
            "floating-width": 50,
            "open-action-strategy": "sourceWindow"
        },
        "simplify": {
            "file-child-template": "[selection | clip | 1] [indent][icon | 1] [filename growRight 1 omitCenter 1]",
            "width": 50,
            "sources": [
                {
                    "name": "file",
                    "expand": true
                }
            ]
        },
        "buffer": {
            "sources": [
                {
                    "name": "buffer",
                    "expand": true
                }
            ]
        }
    },
    "semanticTokens.filetypes": [
        "*"
    ],
    "coc.preferences.currentFunctionSymbolAutoUpdate": true,
    "signature.floatConfig": {
        "borderhighlight": "Constant",
        "highlight": "CocHintFloat",
        "border": true
    },
    "signature.triggerSignatureWait": 500,
    "hover.floatConfig": {
        "borderhighlight": "Constant",
        "highlight": "CocHintFloat",
        "border": true,
        "maxWidth": 80
    },
    "dialog.floatHighlight": "GruvboxOrange",
    "dialog.floatBorderHighlight": "GruvboxBlueBold",
    "suggest.floatConfig": {
        "borderhighlight": "Constant",
        "highlight": "CocHintFloat",
        "border": true,
        "maxWidth": 80,
        "winblend": 15
    },
    "suggest.completionItemKindLabels": {
        "keyword": "\uf1de",
        "variable": "\ue79b",
        "value": "\uf89f",
        "operator": "\u03a8",
        "constructor": "\uf0ad",
        "function": "\u0192",
        "reference": "\ufa46",
        "constant": "\uf8fe",
        "method": "\uf09a",
        "struct": "\ufb44",
        "class": "\uf0e8",
        "interface": "\uf417",
        "text": "\ue612",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "module": "\uf40d",
        "color": "\ue22b",
        "property": "\ue624",
        "field": "\uf9be",
        "unit": "\uf475",
        "event": "\ufacd",
        "file": "\uf723",
        "folder": "\uf114",
        "snippet": "\ue60b",
        "typeParameter": "\uf728",
        "default": "\uf29c"
    },
    "clangd.path": "~/clangd_16.0.2/bin/clangd",
    "Lua.telemetry.enable": false
}
